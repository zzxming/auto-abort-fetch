{
	"compilerOptions": {
		"target": "es5",
		"module": "CommonJS",
		"lib" : ["es2015", "es2016", "es2017", "dom"],	
		"outDir": "./dist",
		"strictNullChecks": true,
		"removeComments": true,
		"noImplicitAny":true,
		"noImplicitThis": true,
		"declaration":  true,
		"sourceMap": true,
		"paths": {
			"@/*": ["src/*"]
		},
		"baseUrl": "."
	},
	"include": ["./src"],
}